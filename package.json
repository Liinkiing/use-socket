  "keywords": [
    "hooks",
    "socketio",
    "socket.io",
    "realtime",
    "react",
    "typescript"
  ],
  "pre-commit": [
    "docs",
    "commit-docs"
  ],
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "@types/socket.io-client": "^1.4.32",
    "pre-commit": "^1.2.2",
    "tslint": "^5.13.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "socket.io-client": "^2.0.0"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  }
